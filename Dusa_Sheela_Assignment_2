      "source": [
        "import nltk\n",
        "nltk.download('averaged_perceptron_tagger')\n",
        "nltk.download('maxent_ne_chunker')\n",
        "nltk.download('words')\n",
        "from nltk import ne_chunk\n",
        "import pandas as pd\n",
        "from nltk import pos_tag, word_tokenize\n",
        "from collections import Counter\n",
        "\n",
        "# Load the cleaned dataset\n",
        "df = pd.read_csv(\"cleaned_movie_reviews.csv\")\n",
        "\n",
        "# Function to perform POS tagging and count POS tags\n",
        "def count_pos_tags(text):\n",
        "    # Tokenize the text\n",
        "    words = word_tokenize(text)\n",
        "    # Perform POS tagging\n",
        "    pos_tags = pos_tag(words)\n",
        "    # Count POS tags\n",
        "    pos_counts = Counter(tag for word, tag in pos_tags)\n",
        "    # Return POS counts\n",
        "    return pos_counts\n",
        "\n",
        "# Function to calculate total number of Noun, Verb, Adjective, Adverb\n",
        "def calculate_pos_totals(df):\n",
        "    # Initialize counters\n",
        "    noun_count = 0\n",
        "    verb_count = 0\n",
        "    adj_count = 0\n",
        "    adv_count = 0\n",
        "\n",
        "    # Iterate over each row in the dataframe\n",
        "    for index, row in df.iterrows():\n",
        "        # Get the cleaned review text\n",
        "        cleaned_review = row['Review']\n",
        "        # Perform POS tagging and count POS tags\n",
        "        pos_counts = count_pos_tags(cleaned_review)\n",
        "        # Update counters\n",
        "        noun_count += pos_counts['NN'] + pos_counts['NNS'] + pos_counts['NNP'] + pos_counts['NNPS']\n",
        "        verb_count += pos_counts['VB'] + pos_counts['VBD'] + pos_counts['VBG'] + pos_counts['VBN'] + pos_counts['VBP'] + pos_counts['VBZ']\n",
        "        adj_count += pos_counts['JJ'] + pos_counts['JJR'] + pos_counts['JJS']\n",
        "        adv_count += pos_counts['RB'] + pos_counts['RBR'] + pos_counts['RBS']\n",
        "\n",
        "    # Return POS totals\n",
        "    return noun_count, verb_count, adj_count, adv_count\n",
        "\n",
        "# Calculate total number of Noun, Verb, Adjective, Adverb\n",
        "noun_total, verb_total, adj_total, adv_total = calculate_pos_totals(df)\n",
        "\n",
        "# Print the results\n",
        "print(\"Total number of Nouns:\", noun_total)\n",
        "print(\"Total number of Verbs:\", verb_total)\n",
        "print(\"Total number of Adjectives:\", adj_total)\n",
        "print(\"Total number of Adverbs:\", adv_total)\n",
        "\n",
        "\n",
        "\n",
        "# Load the cleaned dataset\n",
        "df = pd.read_csv(\"cleaned_movie_reviews.csv\")\n",
        "\n",
        "# Function to extract product names using regular expressions\n",
        "def extract_products(text):\n",
        "    # Define regex pattern to match product names\n",
        "    pattern = r'\\b(?:[A-Z][a-z]*(?:\\s+[A-Z][a-z]*)+)\\b'  # Assumes product names start with a capital letter\n",
        "\n",
        "    # Find all matches of the pattern in the text\n",
        "    products = re.findall(pattern, text)\n",
        "    return products\n",
        "\n",
        "# Function to extract dates using regular expressions\n",
        "def extract_dates(text):\n",
        "    # Define regex pattern to match dates in the format MM/DD/YYYY or MM-DD-YYYY\n",
        "    pattern = r'\\b\\d{1,2}[/-]\\d{1,2}[/-]\\d{2,4}\\b'\n",
        "\n",
        "    # Find all matches of the pattern in the text\n",
        "    dates = re.findall(pattern, text)\n",
        "    return dates\n",
        "\n",
        "# Function to perform Named Entity Recognition (NER) and count entities\n",
        "def count_entities(text):\n",
        "    # Tokenize the text\n",
        "    words = word_tokenize(text)\n",
        "    # Perform Part-of-Speech (POS) tagging\n",
        "    pos_tags = pos_tag(words)\n",
        "    # Perform Named Entity Recognition (NER)\n",
        "    ner_tags = ne_chunk(pos_tags)\n",
        "\n",
        "    # Initialize counters for each entity type\n",
        "    person_count = 0\n",
        "    organization_count = 0\n",
        "    location_count = 0\n",
        "\n",
        "    # Lists to store words for each entity type\n",
        "    person_words = []\n",
        "    organization_words = []\n",
        "    location_words = []\n",
        "\n",
        "    # Iterate over each chunk in the NER result\n",
        "    for chunk in ner_tags:\n",
        "        # Check if the chunk is a Named Entity (NE)\n",
        "        if hasattr(chunk, 'label'):\n",
        "            # Determine the type of NE and update the corresponding counter\n",
        "            if chunk.label() == 'PERSON':\n",
        "                person_count += 1\n",
        "                person_words.extend([word for word, tag in chunk.leaves()])\n",
        "            elif chunk.label() == 'ORGANIZATION':\n",
        "                organization_count += 1\n",
        "                organization_words.extend([word for word, tag in chunk.leaves()])\n",
        "            elif chunk.label() == 'GPE':  # Location\n",
        "                location_count += 1\n",
        "                location_words.extend([word for word, tag in chunk.leaves()])\n",
        "\n",
        "    # Return counts of each entity type and the words\n",
        "    return person_count, organization_count, location_count, person_words, organization_words, location_words\n",
        "\n",
        "# Function to calculate count of each entity type in the dataframe\n",
        "def calculate_entity_counts(df):\n",
        "    # Initialize counters\n",
        "    person_total = 0\n",
        "    organization_total = 0\n",
        "    location_total = 0\n",
        "    product_total = 0\n",
        "    date_total = 0\n",
        "\n",
        "    # Lists to store words for each entity type\n",
        "    all_person_words = []\n",
        "    all_organization_words = []\n",
        "    all_location_words = []\n",
        "    all_product_words = []\n",
        "    all_date_words = []\n",
        "\n",
        "    # Iterate over each row in the dataframe\n",
        "    for index, row in df.iterrows():\n",
        "        # Get the cleaned review text\n",
        "        cleaned_review = row['Review']\n",
        "\n",
        "        # Extract product names and count\n",
        "        products = extract_products(cleaned_review)\n",
        "        product_total += len(products)\n",
        "        all_product_words.extend(products)\n",
        "\n",
        "        # Extract dates and count\n",
        "        dates = extract_dates(cleaned_review)\n",
        "        date_total += len(dates)\n",
        "        all_date_words.extend(dates)\n",
        "\n",
        "        # Perform Named Entity Recognition (NER) and count entities\n",
        "        person_count, organization_count, location_count, person_words, organization_words, location_words = count_entities(cleaned_review)\n",
        "        # Update total counts\n",
        "        person_total += person_count\n",
        "        organization_total += organization_count\n",
        "        location_total += location_count\n",
        "\n",
        "        # Extend lists of words\n",
        "        all_person_words.extend(person_words)\n",
        "        all_organization_words.extend(organization_words)\n",
        "        all_location_words.extend(location_words)\n",
        "\n",
        "    # Return total counts of each entity type and words\n",
        "    return person_total, organization_total, location_total, product_total, date_total, all_person_words, all_organization_words, all_location_words, all_product_words, all_date_words\n",
        "\n",
        "# Calculate count of each entity type\n",
        "person_total, organization_total, location_total, product_total, date_total, all_person_words, all_organization_words, all_location_words, all_product_words, all_date_words = calculate_entity_counts(df)\n",
        "print(\"\\n\")\n",
        "# Print the results\n",
        "print(\"Total number of Person names:\", person_total)\n",
        "print(\"Total number of Organizations:\", organization_total)\n",
        "print(\"Total number of Locations:\", location_total)\n",
        "print(\"Total number of Products:\", product_total)\n",
        "print(\"Total number of Dates:\", date_total)\n",
        "print(\"\\n\")\n",
        "# Print words and their frequency for each category\n",
        "print(\"\\nPerson names:\")\n",
        "print(Counter(all_person_words).most_common())\n",
        "print(\"\\nOrganizations:\")\n",
        "print(Counter(all_organization_words).most_common())\n",
        "print(\"\\nLocations:\")\n",
        "print(Counter(all_location_words).most_common())\n",
        "print(\"\\nProduct names:\")\n",
        "print(Counter(all_product_words).most_common())\n",
        "print(\"\\nDates:\")\n",
        "print(Counter(all_date_words).most_common())\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import spacy\n",
        "from nltk.tokenize import sent_tokenize\n",
        "\n",
        "cleaned_file_path = 'cleaned_movie_reviews.csv'\n",
        "df = pd.read_csv(cleaned_file_path)\n",
        "\n",
        "# Tokenizing the text\n",
        "sentences = df['Review'].tolist()\n",
        "\n",
        "# Loading English language model for spaCy..\n",
        "nlp = spacy.load('en_core_web_sm')\n",
        "\n",
        "# Function which prints constituency parsing and dependency parsing trees for each sentence\n",
        "def print_parsing_trees(sentence):\n",
        "    doc = nlp(sentence)\n",
        "    print(\"\\nSentence:\", sentence)\n",
        "    print(\"\\nConstituency parsing tree:\")\n",
        "    for token in doc:\n",
        "        print(token.text, token.dep_, token.head.text)\n",
        "    print(\"\\nDependency parsing tree:\")\n",
        "    for chunk in doc.noun_chunks:\n",
        "        print(chunk.text, \"-->\", chunk.root.text)\n",
        "\n",
        "# calling the function for each sentence\n",
        "for sentence in sentences:\n",
        "    print_parsing_trees(sentence)\n"
      ],
