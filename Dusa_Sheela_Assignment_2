      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Extracting reviews for 'Mission: Impossible - Dead Reckoning Part One'...\n",
            "Total reviews collected for 'Mission: Impossible - Dead Reckoning Part One': 1000\n",
            "Reviews saved to movie_reviews.csv\n"
          ]
        }
      ],
      "source": [
        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "import csv\n",
        "\n",
        "def gather_reviews(movie_id=\"tt9603212\", count=1000):\n",
        "    movie_name = \"Mission: Impossible - Dead Reckoning Part One\"\n",
        "    url = f\"https://www.imdb.com/title/{movie_id}/reviews\"\n",
        "    agent = \"Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/58.0.3029.110 Safari/537.3\"\n",
        "\n",
        "    print(f\"Extracting reviews for '{movie_name}'...\")\n",
        "\n",
        "    reviews = []\n",
        "    page = 1\n",
        "\n",
        "    while len(reviews) < count:\n",
        "        page_url = f\"{url}?sort=submissionDate&dir=desc&ratingFilter=0&page={page}\"\n",
        "        response = requests.get(page_url, headers={\"User-Agent\": agent})\n",
        "        if response.status_code == 200:\n",
        "            html = BeautifulSoup(response.text, \"html.parser\")\n",
        "            containers = html.find_all(\"div\", class_=\"text show-more__control\")\n",
        "            if not containers:\n",
        "                break\n",
        "            for container in containers:\n",
        "                text = container.get_text(strip=True)\n",
        "                reviews.append(text)\n",
        "            page += 1\n",
        "        else:\n",
        "            print(\"Failed to fetch data.\")\n",
        "            break\n",
        "\n",
        "    print(f\"Total reviews collected for '{movie_name}': {len(reviews)}\")\n",
        "\n",
        "    filename = \"movie_reviews.csv\"\n",
        "    with open(filename, mode=\"w\", newline=\"\", encoding=\"utf-8\") as file:\n",
        "        writer = csv.writer(file)\n",
        "        writer.writerow([\"Review\"])\n",
        "        for review in reviews:\n",
        "            writer.writerow([review])\n",
        "\n",
        "    print(\"Reviews saved to movie_reviews.csv\")\n",
        "\n",
        "# Example usage:\n",
        "gather_reviews(movie_id=\"tt9603212\", count=1000)\n"
      ]
    },
    {
