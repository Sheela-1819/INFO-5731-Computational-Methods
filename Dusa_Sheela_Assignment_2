            "\n",
            "\n",
            "Cleaned data saved to cleaned_movie_reviews.csv\n"
          ]
        }
      ],
      "source": [
        "import nltk\n",
        "nltk.download('stopwords')\n",
        "nltk.download('punkt')\n",
        "nltk.download('wordnet')\n",
        "import pandas as pd\n",
        "import re\n",
        "from nltk.corpus import stopwords\n",
        "from nltk.stem import PorterStemmer, WordNetLemmatizer\n",
        "from nltk.tokenize import word_tokenize\n",
        "\n",
        "# Function to remove noise, such as special characters and punctuations\n",
        "def remove_special_characters(text):\n",
        "    cleaned_text = re.sub(r'[^a-zA-Z\\s]', '', text)\n",
        "    return cleaned_text\n",
        "\n",
        "# Function to remove numbers\n",
        "def remove_numbers(text):\n",
        "    cleaned_text = re.sub(r'\\d+', '', text)\n",
        "    return cleaned_text\n",
        "\n",
        "# Function to remove stopwords by using the stopwords list\n",
        "def remove_stopwords(text):\n",
        "    stop_words = set(stopwords.words('english'))\n",
        "    words = word_tokenize(text)\n",
        "    cleaned_text = ' '.join([word for word in words if word.lower() not in stop_words])\n",
        "    return cleaned_text\n",
        "\n",
        "# Function to lowercase all texts\n",
        "def lowercase_text(text):\n",
        "    return text.lower()\n",
        "\n",
        "# Function to stem the text\n",
        "def stem_text(text):\n",
        "    stemmer = PorterStemmer()\n",
        "    stemmed_words = [stemmer.stem(word) for word in word_tokenize(text)]\n",
        "    return ' '.join(stemmed_words)\n",
        "\n",
        "# Function to lemmatize the text\n",
        "def lemmatize_text(text):\n",
        "    lemmatizer = WordNetLemmatizer()\n",
        "    lemmatized_words = [lemmatizer.lemmatize(word) for word in word_tokenize(text)]\n",
        "    return ' '.join(lemmatized_words)\n",
        "\n",
        "# Load the dataset\n",
        "df = pd.read_csv(\"movie_reviews.csv\")\n",
        "\n",
        "# Apply text cleaning functions to the 'Review' column\n",
        "for index, row in df.iterrows():\n",
        "    review = row['Review']\n",
        "\n",
        "    # Print original review\n",
        "    print(f\"Original Review {index + 1}:\")\n",
        "    print(review)\n",
        "\n",
        "    # Remove noise\n",
        "    review = remove_special_characters(review)\n",
        "    print(f\"Review after removing noise:\")\n",
        "    print(review)\n",
        "\n",
        "    # Remove numbers\n",
        "    review = remove_numbers(review)\n",
        "    print(f\"Review after removing numbers:\")\n",
        "    print(review)\n",
        "\n",
        "    # Remove stopwords\n",
        "    review = remove_stopwords(review)\n",
        "    print(f\"Review after removing stopwords:\")\n",
        "    print(review)\n",
        "\n",
        "    # Lowercase text\n",
        "    review = lowercase_text(review)\n",
        "    print(f\"Review after lowercase conversion:\")\n",
        "    print(review)\n",
        "\n",
        "    # Stem text\n",
        "    review_stemmed = stem_text(review)\n",
        "    print(f\"Review after stemming:\")\n",
        "    print(review_stemmed)\n",
        "\n",
        "    # Lemmatize text\n",
        "    review_lemmatized = lemmatize_text(review)\n",
        "    print(f\"Review after lemmatization:\")\n",
        "    print(review_lemmatized)\n",
        "\n",
        "    # Print newline for readability\n",
        "    print(\"\\n\")\n",
        "\n",
        "# Save the cleaned data with new columns to a new CSV file\n",
        "df.to_csv(\"cleaned_movie_reviews.csv\", index=False)\n",
        "\n",
        "print(\"Cleaned data saved to cleaned_movie_reviews.csv\")\n"
      ]
    },
